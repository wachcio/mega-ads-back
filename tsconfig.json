{
    "compilerOptions": {
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "es6",
        "downlevelIteration": true,
        "lib": ["es6"],
        "outDir": "dist",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "Node",
        "esModuleInterop": true
    }
}
